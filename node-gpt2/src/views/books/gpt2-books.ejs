<%- include ('../partials/header.ejs') %>
    <%- include ('../partials/navigation.ejs') %>

        <!-- ======= Pricing Section ======= -->
        <section id="pricing" class="pricing section-bg">
            <div class="container">

                <div class="section-title" data-aos="fade-up">
                    <h2>GPT2</h2>
                    <p style="color: #333;">Algorithm response time
                        <%= data.timeGPT2 %> seconds
                       
                    </p><br>
                    <h4> <%= sendtitle %></h4>
                </div>

        
                <div class="row">


                    <div class="col-lg-6 col-md-6 mt-4 mt-lg-0">
                        <div class="box" data-aos="zoom-in" data-aos-delay="100">
                            <h3>Abstract Original</h3>
                            <div class="card " style="color: #333;border: none;text-align: justify;">
                                <%= sendabstract %>
                                    <div class="card-footer text-muted mt-5">
                                        <%= data.num_words_original %> words found
                                    </div>


                            </div>

                        </div>
                    </div>


                    <div class="col-lg-6 col-md-6 mt-4 mt-lg-0">
                        <div class="box" data-aos="zoom-in" data-aos-delay="100">
                            <h3>Abstract GPT2</h3>
                            <div class="card " style="color: #333; border: none;text-align: justify;">
                                <%= data.new_abstract %>

                                    <div class="card-footer text-muted mt-5">
                                        <%= data.num_words_gpt2 %> words found
                                    </div>
                            </div>
                        </div>
                    </div>

                </div>



                <!-- ======= Portfolio Section ======= -->
                <section id="portfolio" class="portfolio">
                    <div class="container">

                        <div class="section-title" data-aos="fade-up">
                            <h2>Results</h2>
                            <p style="color: #333;">Below are the similarities found between the original abstract and
                                the abstract generated by GPT2</p>
                        </div>


                        <div class="row portfolio-container" data-aos="fade-up" data-aos-delay="200">

                            <div class="col-lg-6 col-md-6 portfolio-item filter-web">
                                <div class="portfolio-wrap">
                                    <img src="/assets/img/jackard.png" class="img-fluid" alt="">
                                    <div class="portfolio-info">
                                        <h4 style="color:transparent">Jackard similarity</h4>
                                        <h5 style="color:white">
                                            <%= data.simi_jaccard %>
                                        </h5>


                                    </div>
                                </div>
                            </div>


                            <div class="col-lg-6 col-md-6 portfolio-item filter-web">
                                <div class="portfolio-wrap">
                                    <img src="/assets/img/cosine.png" class="img-fluid" alt="">
                                    <div class="portfolio-info">
                                        <h4 style="color:transparent">Cosine Similarity</h4>
                                        <h5 style="color:white">
                                            <%= data.simi_coseno %>
                                        </h5>


                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </section>
                <!-- End Portfolio Section -->

            </div>


            <div class="text-center" style="text-align: center; display: block;">
                <a type="button" href="javascript: history.go(-1)" class="btn"
                    style="box-shadow: 0px 2px 15px rgba(41, 150, 114, 0.89);background: #7cc576; color: white;">RETURN</a>
            </div>



        </section>

        <!-- End Pricing Section -->

        <%- include ('../partials/footer.ejs') %>